  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nknorg/nkn-wallet-js.git"
  },
  "keywords": [
    "NKN",
