  "browserify": {
    "transform": [
      [
        "browserify-replace",
        {
          "replace": [
            {
              "from": "var global = Function\\('return this'\\)\\(\\);",
              "to": "var global = (function(){ return this }).call(null);"
            }
          ]
        }
      ]
    ]
  },
  "dependencies": {
    "axios": "0.18.0",
    "base-x": "3.0.4",
    "buffer": "^5.2.1",
    "crypto-js": "3.1.9-1",
    "decimal.js": "10.0.1",
    "es6-promise": "4.2.4",
    "google-protobuf": "^3.7.1",
    "is": "3.2.1",
    "tweetnacl": "^1.0.1"
  },
  "devDependencies": {
    "browserify-replace": "^1.0.0",
    "grunt": "^1.0.2",
    "grunt-browserify": "^5.3.0",
    "grunt-contrib-uglify-es": "^3.3.0",
    "uglify-js": "^3.3.24"
  }
}
